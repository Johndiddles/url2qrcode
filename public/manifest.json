{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "URL2QRCODE",
  "description": "Converts browser urls to QR Codes",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
